Vagrant.configure("2") do |config|
  config.vm.box = "hashicorp/bionic64"
  config.vm.define "vmServidor"
  (8081..8086).each do |port|
    config.vm.network "forwarded_port",ip: "192.168.56.200", guest: port, host: port
  end
  config.vm.hostname = "vmServidor"
  config.vm.provider "virtualbox" do |vb|
    vb.memory = "512"
  end    
  config.vm.provision "file", source: "./server.c", destination: "~/server.c"
  config.vm.provision "shell", path: "script.sh"
end
